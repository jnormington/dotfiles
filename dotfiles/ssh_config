Host github.com
  HostName github.com
  IdentityFile ~/.ssh/github_rsa
  PreferredAuthentications publickey
  User git

Host bitbucket
  HostName bitbucket.org
  IdentityFile ~/.ssh/bitbucket_rsa
  PreferredAuthentications publickey
  User git


